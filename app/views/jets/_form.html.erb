<div class="container-form">
  <h1>New Jet</h1>
  <%= simple_form_for(@jet) do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
  </div>
  <div class="form-actions">
    <%= f.submit "Submit Jet", class: "btn btn-dark" %>
  </div>
  <% end %>
</div>
